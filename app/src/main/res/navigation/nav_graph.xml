<?xml version="1.0" encoding="utf-8"?>
    <navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/nav_graph"
        app:startDestination="@id/homeFragment">

        <fragment
            android:id="@+id/homeFragment"
            android:name="com.example.labs.ui.HomeFragment"
            android:label="HomeFragment">
            <action
                android:id="@+id/action_homeFragment_to_lab1Fragment"
                app:destination="@id/firstLabFragment" />
            <action
                android:id="@+id/action_homeFragment_to_secondLabFragment"
                app:destination="@id/secondLabFragment" />
            <action
                android:id="@+id/action_homeFragment_to_sixth"
                app:destination="@id/sixth" />
            <action
                android:id="@+id/action_homeFragment_to_third"
                app:destination="@id/third" />
            <action
                android:id="@+id/action_homeFragment_to_fifth"
                app:destination="@id/fifth" />
            <action
                android:id="@+id/action_homeFragment_to_eighth"
                app:destination="@id/eighth" />
            <action
                android:id="@+id/action_homeFragment_to_fourth"
                app:destination="@id/fourth" />
            <action
                android:id="@+id/action_homeFragment_to_seven"
                app:destination="@id/seven" />
        </fragment>
    <fragment
        android:id="@+id/firstLabFragment"
        android:name="com.example.labs.ui.labFragments.lab1.First"
        android:label="fragment_first_lab" />
    <fragment
        android:id="@+id/secondLabFragment"
        android:name="com.example.labs.ui.labFragments.lab2.Second"
        android:label="fragment_second_lab" />
    <fragment
        android:id="@+id/eighth"
        android:name="com.example.labs.ui.labFragments.lab8.Eighth"
        android:label="Eighth" />
    <fragment
        android:id="@+id/fifth"
        android:name="com.example.labs.ui.labFragments.lab5.Fifth"
        android:label="Fifth" />
    <fragment
        android:id="@+id/sixth"
        android:name="com.example.labs.ui.labFragments.lab6.Sixth"
        android:label="Sixth" />
    <fragment
        android:id="@+id/third"
        android:name="com.example.labs.ui.labFragments.lab3.Third"
        android:label="Third" />
    <fragment
        android:id="@+id/fourth"
        android:name="com.example.labs.ui.labFragments.lab4.Fourth"
        android:label="Fourth" >
        <action
            android:id="@+id/action_fourth_to_checkForOpenKeyFragment"
            app:destination="@id/checkForOpenKeyFragment" />
    </fragment>
    <fragment
        android:id="@+id/checkForOpenKeyFragment"
        android:name="com.example.labs.ui.labFragments.lab4.CheckForOpenKeyFragment"
        android:label="CheckForOpenKeyFragment" />
    <fragment
        android:id="@+id/seven"
        android:name="com.example.labs.ui.labFragments.lab7.Seven"
        android:label="Seven">
        <action
            android:id="@+id/action_seven_to_registrationFragment"
            app:destination="@id/registrationFragment" />
    </fragment>
    <fragment
        android:id="@+id/registrationFragment"
        android:name="com.example.labs.ui.labFragments.lab7.RegistrationFragment"
        android:label="fragment_registration"
        tools:layout="@layout/fragment_registration" />

</navigation>
